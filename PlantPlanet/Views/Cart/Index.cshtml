@{
    @using Microsoft.AspNetCore.Http
    @inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor

    if (SessionHelper.GetObjectFromJson<List<CartProduct>>(HttpContextAccessor.HttpContext.Session, "cart") as IList<CartProduct> == null)
    {
        <div>dsd</div>
    }
    else
    {
        @foreach (var product in SessionHelper.GetObjectFromJson<List<CartProduct>>(HttpContextAccessor.HttpContext.Session, "cart") as IList<CartProduct>)
        {
            <div>@product.product.Name</div>
            <div>@product.quantity</div>
        }
    }
}